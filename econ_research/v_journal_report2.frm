source=SELECT\n    journal.jour_id,\n    journal.jour_owner_citizenid AS citizenid,\n    journal.jour_owner_prename AS prename,\n    journal.jour_owner_firstname AS firstname,\n    journal.jour_owner_lastname AS lastname,\n    journal.department_name AS department_name,\n    journal.jour_ratio,\n    journal.jour_study,\n    journal.jour_tier,\n    journal.jour_journal,\n    journal.jour_value,\n    journal.jour_volume_issue,\n    journal.jour_page,\n    journal.jour_link,\n    journal.jour_date_avaliable,\n    journal.jour_fiscalyear,\n    journal.jour_attach,\n    journal.jour_detail,\n    journal.jour_notes,\n    journal.jour_create_datetime,\n    journal.jour_status,\n    journal.jour_editor,\n    journal.jour_lastupdate\nFROM\n    journal\nWHERE\n    journal.jour_status = \'enable\'\nUNION ALL\nSELECT\n    journal.jour_id,\n    co_worker.cow_citizenid,\n    co_worker.cow_prename,\n    co_worker.cow_firstname,\n    co_worker.cow_lastname,\n    co_worker.department_name,\n    co_worker.cow_ratio,\n    journal.jour_study,\n    journal.jour_tier,\n    journal.jour_journal,\n    journal.jour_value,\n    journal.jour_volume_issue,\n    journal.jour_page,\n    journal.jour_link,\n    journal.jour_date_avaliable,\n    journal.jour_fiscalyear,\n    journal.jour_attach,\n    journal.jour_detail,\n    journal.jour_notes,\n    co_worker.cow_create_datetime,\n    co_worker.cow_status,\n    co_worker.cow_editor,\n    co_worker.cow_lastupdate\nFROM\n    journal\nLEFT JOIN co_worker ON co_worker.cow_ref_id = journal.jour_id\nWHERE\n    co_worker.cow_ref_table = \'journal\'\nORDER BY\n    jour_id\nDESC

source=SELECT\n    proceeding.pro_id AS pro_id,\n    proceeding.pro_owner_citizenid AS citizenid,\n    proceeding.pro_owner_prename AS prename,\n    proceeding.pro_owner_firstname AS firstname,\n    proceeding.pro_owner_lastname AS lastname,\n    proceeding.department_name AS department_name,\n    proceeding.pro_ratio AS pro_ratio,\n    proceeding.pro_study AS pro_study,\n    proceeding.pro_conf AS pro_conf,\n    proceeding.pro_conf_owner AS pro_conf_owner,\n    proceeding.pro_location AS pro_location,\n    proceeding.pro_date_begin AS pro_date_begin,\n    proceeding.pro_date_end AS pro_date_end,\n    proceeding.pro_volume_issue AS pro_volume_issue,\n    proceeding.pro_page AS pro_page,\n    proceeding.pro_link AS pro_link,\n    proceeding.pro_tier AS pro_tier,\n    proceeding.pro_fiscalyear AS pro_fiscalyear,\n    proceeding.pro_attach AS pro_attach,\n    proceeding.pro_detail AS pro_detail,\n    proceeding.pro_notes AS pro_notes,\n    proceeding.pro_create_datetitme AS pro_create_datetitme,\n    proceeding.pro_status AS pro_status,\n    proceeding.pro_editor AS pro_editor,\n    proceeding.pro_lastupdate AS pro_lastupdate\nFROM\n    proceeding\nWHERE\n    proceeding.pro_status = \'enable\'\nUNION ALL\nSELECT\n    proceeding.pro_id AS pro_id,\n    co_worker.cow_citizenid AS citizenid,\n    co_worker.cow_prename AS prename,\n    co_worker.cow_firstname AS firstname,\n    co_worker.cow_lastname AS lastname,\n    co_worker.department_name AS department_name,\n    co_worker.cow_ratio AS cow_ratio,\n    proceeding.pro_study AS pro_study,\n    proceeding.pro_conf AS pro_conf,\n    proceeding.pro_conf_owner AS pro_conf_owner,\n    proceeding.pro_location AS pro_location,\n    proceeding.pro_date_begin AS pro_date_begin,\n    proceeding.pro_date_end AS pro_date_end,\n    proceeding.pro_volume_issue AS pro_volume_issue,\n    proceeding.pro_page AS pro_page,\n    proceeding.pro_link AS pro_link,\n    proceeding.pro_tier AS pro_tier,\n    proceeding.pro_fiscalyear AS pro_fiscalyear,\n    proceeding.pro_attach AS pro_attach,\n    proceeding.pro_detail AS pro_detail,\n    proceeding.pro_notes AS pro_notes,\n    co_worker.cow_create_datetime AS cow_create_datetime,\n    co_worker.cow_status AS cow_status,\n    co_worker.cow_editor AS cow_editor,\n    co_worker.cow_lastupdate AS cow_lastupdate\nFROM\n    proceeding\nLEFT JOIN co_worker ON co_worker.cow_ref_id = proceeding.pro_id\nWHERE\n    co_worker.cow_ref_table = \'proceeding\'\nORDER BY\n    pro_id\nDESC
